roundedBox()
	background-color: #F5F5F5;
	border 1px solid #393C3F
	border-radius: 4px;	
	padding 20px
	
#leftbar
	position: absolute
	roundedBox()
	top:55px
	bottom:25%
	left: 15px
	width: 20%
	#vidYourself
		width: 100%
#participants
	position: fixed
	bottom:0
	width: 100%
	height: 25%
	left: 15px
	.tab
		background-color: #485E7B
		border 1px solid #393C3F
		border-radius: 4px
		margin-top: 16px
		padding: 5px
		height: 90%
		width:230px
		display:inline-block
		text-align: center
		position: relative
		.name
			overflow: hidden;
			white-space: nowrap;
	.tab-medic, .tab-patient
		top:-10px
	.tab:hover
		border 1px solid #FFF
	.connected
		background-color: #044DAB
		color: #FFF
		
.mini-video
	width: 100%
	overflow: hidden
	
#mainVideo
	position: absolute;
	top:55px;
	bottom: 25%;
	left: 23.5%
	right: 20px
	.video
		margin: 20px
		margin-top:0
		height:90%
		width:45%
		display: inline-block
		div
			width:100%
			height:100%
	label
		color: white
		text-shadow: -1px 1px 0 #373A3D;
		font-size:1.25em
		position:absolute
		top:10px
		z-index:9999
		width: 100%
		text-align: center
		
		
